delimiter | 
create procedure formInsertCode(libelleCours varchar(256), libCodeBaps varchar(256), libCodeRayhane varchar(256))
BEGIN 
	insert ignore into codeBaps(libelleCodeBaps) values(libCodeBaps);

	insert ignore into cours(libelle, codeBaps) values(libelleCours, libCodeBaps);

	insert ignore into codeRayhane(codeRayhane, codebaps) values(libCodeRayhane, libCodeBaps);

	update ignore codeBaps set libelleCours = libelleCours, codeRayhane = libCodeRayhane where id = (select max(id)); 
end |
delimiter ;
	



delimiter | 
create procedure formInsertFiles(emplacementCoursSource varchar(256), emplacementSlide varchar(256), emplacementSupportCoursGen varchar(256), libCodeBaps varchar(256), libCodeRayhane varchar(256), csv varchar(256))
BEGIN 

	insert ignore into supportCoursSource(emplacement, codeBaps, codeRayhane) values (emplacementCoursSource, libCodeBaps, libCodeRayhane);

	insert ignore into supportCoursGen(emplacement, supportCoursSource, codeBaps, codeRayhane) values(emplacementSupportCoursGen, emplacementCoursSource, libCodeBaps, libCodeRayhane);

	insert into csv(emplacement) values(csv);

	insert into slide(emplacementFichier, supportCoursSource, supportCoursGen, codeBaps, codeRayhane, emplacementCsv) values (emplacementSlide, emplacementCoursSource, emplacementSupportCoursGen, libCodeBaps, libCodeRayhane, csv); 

	
	update supportCoursSource set slide = emplacementSlide, supportCoursGen = emplacementSupportCoursGen where id = (select max(id) from slide);

	update supportCoursGen set slide = emplacementSlide where id = (select max(id) from slide);
end |
delimiter ;




delimiter |
create procedure insertPdg(libelle varchar(256), emplacement varchar(256))
Begin
	insert ignore into stockerPageDeGarde(emplacement, libelle) values(emplacement , libelle);
end |
delimiter ;




	
